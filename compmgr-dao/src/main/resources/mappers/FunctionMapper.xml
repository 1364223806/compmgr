<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.zut.cs.rolepower.dao.FunctionMapper">
    <select id="getIdById"  parameterType="java.lang.Integer" resultType="java.lang.String">
        select id
        from t_function
        where id=#{0}
    </select>

    <select id="getFunctionById"  parameterType="java.lang.Integer" resultType="java.lang.String">
        select function
        from t_function
        where id=#{0}
    </select>
</mapper>