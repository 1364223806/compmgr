<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.zut.cs.admin.dao.RecordMapper">
<select id="getArrangeIdById" parameterType="java.lang.Integer" resultType="edu.zut.cs.admin.model.Record">
    SELECT arrangeId
    FROM t_record
    WHERE id=#{0}
</select>

<select id="getLabIdById" parameterType="java.lang.Integer" resultType="edu.zut.cs.admin.model.Record">
    SELECT labId
    FROM t_record
    WHERE id=#{0}
</select>

</mapper>